	config PACKAGE_ndppd
		tristate "ndppd................................................... NDP Proxy Daemon"
		default y if DEFAULT_ndppd
		default m if ALL
		select PACKAGE_libstdcpp if USE_LIBSTDCXX
		select PACKAGE_kmod-ipv6
