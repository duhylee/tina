Config:
    menu "Select GStreamer bad modules and libraries"
      depends on PACKAGE_gstreamer1-plugins-bad


    config gst1-plugins-bad-all
      bool "Include all GStreamer bad plugins"
      select PACKAGE_gst1-plugins-bad

    comment "Modules"

    config PACKAGE_gst1-mod-adpcmdec
      prompt "GStreamer adpcmdec module"

     config PACKAGE_gst1-mod-adpcmenc
      prompt "GStreamer adpcmenc module"

     config PACKAGE_gst1-mod-aiff
      prompt "GStreamer aiff module"

     config PACKAGE_gst1-mod-asfmux
      prompt "GStreamer asfmux module"

     config PACKAGE_gst1-mod-autoconvert
      prompt "GStreamer autoconvert module"

     config PACKAGE_gst1-mod-bayer
      prompt "GStreamer bayer module"

     config PACKAGE_gst1-mod-camerabin
      prompt "GStreamer camerabin module"

     config PACKAGE_gst1-mod-debugutilsbad
      prompt "GStreamer debugutilsbad module"

     config PACKAGE_gst1-mod-dvdspu
      prompt "GStreamer dvdspu module"

     config PACKAGE_gst1-mod-fbdevsink
      prompt "GStreamer fbdevsink module"

     config PACKAGE_gst1-mod-waylandsink
      prompt "GStreamer waylandsink module"

     config PACKAGE_gst1-mod-festival
      prompt "GStreamer festival module"

     config PACKAGE_gst1-mod-hls
      prompt "GStreamer hls module"

     config PACKAGE_gst1-mod-frei0r
      prompt "GStreamer frei0r module"

     config PACKAGE_gst1-mod-id3tag
      prompt "GStreamer id3tag module"

     config PACKAGE_gst1-mod-jpegformat
      prompt "GStreamer jpegformat module"

     config PACKAGE_gst1-mod-mpegpsdemux
