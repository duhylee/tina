lcd_x               = 1280
lcd_y               = 320
lcd_width           = 108
lcd_height          = 64
lcd_dclk_freq       = 33
lcd_pwm_used        = 1
lcd_pwm_ch          = 0
lcd_pwm_freq        = 40000
lcd_pwm_pol         = 1
lcd_pwm_max_limit   = 255
lcd_hbp             = 80
lcd_ht              = 1470
lcd_hspw            = 30
lcd_vbp             = 30
lcd_vt              = 360
lcd_vspw            = 2
lcd_frm             = 1
lcd_hv_clk_phase    = 0
lcd_hv_sync_polarity = 0
lcd_gamma_en        = 0
lcd_bright_curve_en = 1
lcd_cmap_en         = 0
lcd_rb_swap = 0
lcdgamma4iep        = 22

