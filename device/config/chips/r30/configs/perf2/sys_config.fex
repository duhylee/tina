;sunxi platform application
;---------------------------------------------------------------------------------------------------------
; 说明： 脚本中的字符串区分大小写，用户可以修改"="后面的数值，但是不要修改前面的字符串
; 描述gpio的形式：Port:端口+组内序号<功能分配><内部电阻状态><驱动能力><输出电平状态>
;---------------------------------------------------------------------------------------------------------

[product]
version = "100"
machine = "evb"

[platform]
eraseflag   = 1

[target]
boot_clock	= 1008
storage_type    = -1

;---------------------------------------------------------------------------------------------------------------
;   power setting
;note:
;dcdc1_vol=1003300, mean set dcdc to 3300mV and enable this dc
;dcdc1_vol=3300,    mean set dcdc to 3300mV but disable this dc

;dcdc1_vol							---1600-3400,100mV/step,vcc-io/vcc-usb/vcc-card/vcc-emmc (ON), default 3.3v
;dcdc2_vol							---500-1300,20mV/step,vdd-cpu ,default 0.9v (ON)
;dcdc3_vol							---500-1300,20mV/step,vdd-cpu ,default 0.9v (ON)
;dcdc4_vol							---500-1300,20mV/step,vdd-gpu ,default 0.9v (ON)
;dcdc5_vol							---800-1840,20mV/step,vcc-dram,default 1.5v (ON)
;dcdc6_vol							---600-1520,20mV/step,vdd-sys/vdd-usb/cpvdd, default 1.5v (ON)
;aldo1_vol							---700-3300,100mV/step,vcc-ve, default 0.9v (ON)
;aldo2_vol							---700-3300,100mV/step,vdd-dram, default 1.8v (ON)
;aldo3_vol							---700-3300,100mV/step,vcc18-PL, default 1.8v/3.3v (ON)
;dldo1_vol							---700-3300,100mV/step,vcc33-wifi, default 3.3v (OFF)
;dldo2_vol							---700-4200,100mV/step,afvcc-csi,vcc-PE, default (OFF)
;dldo3_vol							---700-3300,100mV/step,vcc-PF, default 1.8v/3.3v (OFF)
;dldo4_vol							---700-3300,100mV/step,avdd-csi, default 2.8v (OFF)
;eldo1_vol							---700-1900,50mV/step, vcc-pll,vcc-edp,vcc-dsi,vcc-efuse,vcc18-emmc, default 1.8v (ON)
;eldo2_vol							---700-1900,50mV/step,dvdd-csi, default 3.3v (OFF)
;eldo3_vol							---700-1900,50mV/step,vcc18-wifi, default 1.8v (OFF)
;fldo1_vol							---700-1450,50mV/step,vcc-cpus,default 0.9v (ON)
;fldo2_vol							---700-1450, NA
;gpio0_vol							---700-3300,100mV/step, vcc18-lcd ,default 1.8v (OFF)
;gpio1_vol							---700-3300,100mV/step, vcc-sensor,default 3.3v (off)
;RTCLDO								---1.8v or 3v, default 3.3v (ON)
;-----------------------------------------------------------------------------------------------------------------
[power_sply]
dcdc1_vol                  = 1003300
dcdc2_vol                  = 1000900
dcdc3_vol                  = 1000900
dcdc4_vol                  = 1000900
dcdc6_vol                  = 1000900
;aldo1_vol                  = 900
;aldo2_vol                  = 1800
;aldo3_vol                  = 1800
dldo1_vol                   = 3300
;dldo2_vol                  = 3100
;dldo3_vol                  = 1800
;dldo4_vol                  = 2800
;eldo1_vol                  = 1800
;eldo2_vol                  = 3300
eldo3_vol                   = 1800
gpio0_vol                  = 1001800
;gpio1_vol                  = 3300
ldoio0_vol = 1001800
dc1sw_vol = 1003300
fldo3_vol                  = 900
dcdc7_vol                  = 900

[card_boot]
logical_start   = 40960
sprite_gpio0    =

;---------------------------------------------------------------------------------------------------------
; if 1 == standby_mode, then support super standby;
; else, support normal standby.
;---------------------------------------------------------------------------------------------------------
[pm_para]
standby_mode		= 1

[card0_boot_para]
card_ctrl       = 0
card_high_speed = 1
card_line       = 4
sdc_d1          = port:PF0<2><1><2><default>
sdc_d0          = port:PF1<2><1><2><default>
sdc_clk         = port:PF2<2><1><2><default>
sdc_cmd         = port:PF3<2><1><2><default>
sdc_d3          = port:PF4<2><1><2><default>
sdc_d2          = port:PF5<2><1><2><default>

[card2_boot_para]
card_ctrl       = 2
card_high_speed = 1
card_line       = 8
sdc_ds		= port:PC01<3><1><3><default>
sdc_clk         = port:PC04<3><1><3><default>
sdc_cmd         = port:PC05<3><1><3><default>
sdc_d0          = port:PC06<3><1><3><default>
sdc_d1          = port:PC07<3><1><3><default>
sdc_d2          = port:PC08<3><1><3><default>
sdc_d3          = port:PC09<3><1><3><default>
sdc_d4          = port:PC10<3><1><3><default>
sdc_d5          = port:PC11<3><1><3><default>
sdc_d6          = port:PC12<3><1><3><default>
sdc_d7          = port:PC13<3><1><3><default>
sdc_emmc_rst    = port:PC14<3><1><3><default>
sdc_ex_dly_used = 2
;sdc_io_1v8      = 1
sdc_tm4_hs400_max_freq = 100

[twi_para]
twi_port        = 0
twi_scl         = port:PH14<2><default><default><default>
twi_sda         = port:PH15<2><default><default><default>

[uart_para]
uart_debug_port = 0
uart_debug_tx   = port:PB09<4><1><default><default>
uart_debug_rx   = port:PB10<4><1><default><default>

[jtag_para]
jtag_enable     = 0
jtag_ms         = port:PB0<4><default><default><default>
jtag_ck         = port:PB1<4><default><default><default>
jtag_do         = port:PB2<4><default><default><default>
jtag_di         = port:PB3<4><default><default><default>

[clock]
pll4            = 300
pll6            = 600
pll8            = 360
pll9            = 297
pll10           = 264

;*****************************************************************************
;sdram configuration
;if dram_clk = 0, dram clk set to be default
;if dram_clk != 0, dram clk set to be dram_clk(24*n MHz,n > 12)
