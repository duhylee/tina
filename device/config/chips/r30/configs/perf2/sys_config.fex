[dram_para]
dram_clk      =0
dram_type     =7
dram_zq       =0x3b3bfb
dram_odt_en   =0x31
dram_para1    =0x10e410e4
dram_para2    =0x0000
dram_mr0      =0x1c70
dram_mr1      =0x40
dram_mr2      =0x18
